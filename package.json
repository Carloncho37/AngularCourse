{
  "name": "bases",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -o",
    "build": "ng build",
    "build:href": "ng build --base-href ./",
    "build:github" : "npm run delete:doc-dist && npm run build:href && npm run copy:dist && npm run delete:dist && npm run git:pushrepo ",
    "git:add" : "git add .",
    "git:commit" : "git commit -m 1",
    "git:push" : "git push origin",
    "git:pushrepo" : "npm run git:add && npm run git:commit && npm run git:push",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "http-server": "http-server -o ./docs",
    "delete:docs": "del docs",
    "delete:dist": "del dist",
    "delete:doc-dist": "del doc del dis",
    "copy:dist": "copyfiles dist/bases/* ./docs -f"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.2.8",
    "@angular/common": "^16.2.8",
    "@angular/compiler": "^16.2.8",
    "@angular/core": "^16.2.8",
    "@angular/forms": "^16.2.8",
    "@angular/platform-browser": "^16.2.8",
    "@angular/platform-browser-dynamic": "^16.2.8",
    "@angular/router": "^16.2.8",
    "rxjs": "~7.8.0",
    "uuid": "^9.0.1",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.1",
    "@angular/cli": "~16.2.1",
    "@angular/compiler-cli": "^16.2.8",
    "@types/jasmine": "~4.3.0",
    "@types/uuid": "^9.0.6",
    "copyfiles": "^2.4.1",
    "del-cli": "^5.1.0",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "tslib": "^2.6.2",
    "typescript": "~5.1.3"
  }
}
